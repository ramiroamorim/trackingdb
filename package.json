{
  "name": "backend-mentor-minimal",
  "version": "0.1.0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "worker": "node worker.js",
    "dev": "nodemon src/server.ts",
    "dev:worker": "nodemon src/worker.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:up": "docker-compose -f docker-compose.production.yml up -d",
    "docker:down": "docker-compose -f docker-compose.production.yml down",
    "docker:logs": "docker-compose -f docker-compose.production.yml logs -f",
    "deploy": "git pull && docker-compose -f docker-compose.production.yml down && docker-compose -f docker-compose.production.yml up -d --build"
  },
  "dependencies": {
    "apiip.net": "^1.3.0",
    "axios": "^1.13.2",
    "bullmq": "^5.63.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "ioredis": "^5.8.2",
    "joi": "^17.11.0",
    "pg": "^8.16.3",
    "typescript": "^5.0.0"
  }
}
